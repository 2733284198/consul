<IvyCodemirror @value={{this.value}} @name={{this.name}} @class={{this.class}} @options={{this.options}} @valueUpdated={{action this.onkeyup}} />
<pre><code>{{yield}}</code></pre>
{{#if (and (not this.readonly) (not this.syntax))}}
  <PowerSelect
      @onChange={{action "change"}}
      @selected={{mode}}
      @searchEnabled={{false}}
      @options={{this.modes}} as |mode|>
    {{mode.name}}
  </PowerSelect>
{{/if}}
