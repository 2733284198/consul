{{#if (gt this.items.length 0)}}
  {{#if (eq this.healthCheck.check 'empty') }}
    <dl class={{this.healthCheck.check}}>
      <dt>
        <Tooltip>
          {{capitalize this.type}} Checks
        </Tooltip>
      </dt>
      <dd>No {{this.type}} checks</dd>
    </dl>
  {{else}}
    {{#if (eq this.healthCheck.count this.items.length)}}
    <dl class={{this.healthCheck.check}}>
      <dt>
        <Tooltip>
          {{capitalize this.type}} Checks
        </Tooltip>
      </dt>
      <dd>All {{this.type}} checks {{this.healthCheck.status}}</dd>
    </dl>
    {{else}}
    <dl class={{this.healthCheck.check}}>
      <dt>
        <Tooltip>
          {{capitalize this.type}} Checks
        </Tooltip>
      </dt>
      <dd>{{this.healthCheck.count}}/{{this.items.length}} {{this.type}} checks {{this.healthCheck.status}}</dd>
    </dl>
    {{/if}}
  {{/if}}
{{/if}}